# Agent Aura - Root Environment Configuration Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env file to version control

# ============================================================================
# API Keys (REQUIRED)
# ============================================================================
GEMINI_API_KEY=your_gemini_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here_optional

# ============================================================================
# Model Configuration
# ============================================================================
ORCHESTRATOR_MODEL=gemini-1.5-pro
WORKER_MODEL=gemini-1.5-pro

# ============================================================================
# Data Paths
# ============================================================================
DATA_DIRECTORY=./data
STUDENT_DATA_FILE=./data/student_data.csv
OUTPUT_DIRECTORY=./output

# ============================================================================
# Risk Analysis Thresholds
# ============================================================================
CRITICAL_RISK_THRESHOLD=0.90
HIGH_RISK_THRESHOLD=0.80
MODERATE_RISK_THRESHOLD=0.60
LOW_RISK_THRESHOLD=0.30

# ============================================================================
# Logging Configuration
# ============================================================================
LOG_LEVEL=INFO
LOG_FILE=agent_aura.log

# ============================================================================
# Agent Configuration
# ============================================================================
MAX_ITERATIONS=3
CONFIDENCE_LEVEL=85

# ============================================================================
# ElevenLabs (Voice Interface)
# ============================================================================
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_VOICE_ID=21m00Tcm4TlvDq8ikWAM
