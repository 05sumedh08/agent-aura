# Environment Configuration for Agent Aura
# Copy this file to .env and fill in your actual values

# ============================================================================
# API KEYS
# ============================================================================

# Google Gemini API Key (Required)
# Get your key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# ============================================================================
# MODEL CONFIGURATION
# ============================================================================

# Orchestrator model (main coordinator agent)
ORCHESTRATOR_MODEL=gemini-2.0-flash-exp

# Worker model (sub-agents)
WORKER_MODEL=gemini-1.5-flash

# ============================================================================
# DATA PATHS
# ============================================================================

# Path to student data CSV file
DATA_DIRECTORY=./data
STUDENT_DATA_FILE=./data/student_data.csv

# Output directory for reports and exports
OUTPUT_DIRECTORY=./output

# ============================================================================
# RISK THRESHOLDS
# ============================================================================

CRITICAL_RISK_THRESHOLD=0.90
HIGH_RISK_THRESHOLD=0.80
MODERATE_RISK_THRESHOLD=0.60
LOW_RISK_THRESHOLD=0.30

# ============================================================================
# LOGGING CONFIGURATION
# ============================================================================

LOG_LEVEL=INFO
LOG_FILE=agent_aura.log

# ============================================================================
# AGENT CONFIGURATION
# ============================================================================

MAX_ITERATIONS=3
CONFIDENCE_LEVEL=85

# ============================================================================
# DEPLOYMENT (Optional)
# ============================================================================

# Google Cloud Project ID (for Cloud Run deployment)
# GCP_PROJECT_ID=your-project-id
# GCP_REGION=us-central1

# Agent Engine Configuration
# AGENT_ENGINE_ENDPOINT=your-agent-engine-endpoint
